{
    "name": "cboden/Ratchet"
  , "type": "library"
  , "description": "PHP WebSocket server component library"
  , "keywords": ["WebSockets", "Server", "Ratchet", "Sockets"]
  , "website": "http://socketo.me"
  , "repository": "https://github.com/cboden/Ratchet"
  , "license": "MIT"
  , "authors": [
        {
            "name": "Chris Boden"
          , "email": "cboden@gmail.com"
          , "homepage": "http://res.im"
        }
    ]
  , "autoload": {
        "psr-0": {
            "Ratchet\\Tests": "tests"
          , "Ratchet": "src"
          , "React": "vendor/cboden/react/src"
        }
    }
  , "require": {
        "php": ">=5.3.2"
      , "guzzle/guzzle": "2.5.*"
      , "symfony/http-foundation": "2.1.*"
      , "evenement/evenement": "dev-master"
      , "cboden/react": "dev-master"
    }
  , "repositories": [
        {
            "type": "package"
          , "package": {
                "name": "cboden/react"
              , "version": "dev-master"
              , "source": {
                    "url": "git://github.com/cboden/SocketServer.git"
                  , "type": "git"
                  , "reference": "ratchet"
                }
            }
        }
    ]
}